author = {Beine, Michel and Parsons, Christopher R.},
doi = {10.1111/sjoe.12098},
file = {:C$\backslash$:/Users/Guy/Documents/Mendeley Desktop/Beine, Parsons{\_}2015{\_}Climatic Factors as Determinants of International Migration.pdf:pdf},
