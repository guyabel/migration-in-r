doi = {10.1145/2047296.2047308},
file = {:C$\backslash$:/Users/Guy/Documents/Mendeley Desktop/Naaman{\_}2011{\_}Geographic information from georeferenced social media data.pdf:pdf},
issn = {1946-7729},
journal = {SIGSPATIAL Special},
number = {2},
