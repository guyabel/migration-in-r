@article{Hardle2009,
abstract = {Population forecasts are crucial for many social, political and economic decisions. Official$\backslash$npopulation projections rely in general on deterministic models which use different scenarios$\backslash$nfor future vital rates to indicate uncertainty. However, this technique shows substantial$\backslash$nweak points such as assuming absolute correlations between the demographic components.$\backslash$nIn this paper, we argue that a stochastic projection alternative, with no a priori assumptions$\backslash$nprovides point forecasts and probabilistic prediction intervals for demographic parameters in$\backslash$naddition. Age-sex specific population forecast for Germany is derived through a stochastic$\backslash$npopulation renewal process using forecasts of mortality, fertility and migration. Time series$\backslash$nmodels with demographic restrictions are used to describe immigration, emigration and time$\backslash$nvarying indices of mortality and fertility rates. These models are then used in the simulation$\backslash$nof future vital rates to obtain age-specific population forecast using the cohort-component$\backslash$nmethod. The consequence for the German pension system is discussed. To maintain the$\backslash$nactual average pension level the premium rate of the present system rises at least by 50{\%} as$\backslash$nthe old-age ratio nearly doubles by 2040.},
