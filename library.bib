file = {:C$\backslash$:/Users/Guy/Documents/Mendeley Desktop/Seebens et al.{\_}2015{\_}Global trade will accelerate plant invasions in emerging economies under climate change.pdf:pdf},
issn = {13541013},
journal = {Global Change Biology},
keywords = {Alien vascular plants,Bioinvasion,Climate warming,Global spread,Imperialist dogma,Model,Network of plant invasion},
month = {nov},
number = {11},
pages = {4128--4140},
pmid = {26152518},
title = {{Global trade will accelerate plant invasions in emerging economies under climate change}},
url = {http://doi.wiley.com/10.1111/gcb.13021},
volume = {21},
year = {2015}
}
