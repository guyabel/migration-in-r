journal = {Nature},
keywords = {Age Distribution,Aged,Aged, 80 and over,Aging,Aging: physiology,Emigration and Immigration,Geography,Humans,Internationality,Life Expectancy,Life Expectancy: ethnology,Life Expectancy: trends,Longevity,Middle Aged,Mortality,Mortality: trends,Population Density,Time Factors},
month = {feb},
number = {7179},
pages = {716--9},
