@article{fotheringham1999rsp,
abstract = {This paper investigates the determinants of spatial knowledge and$\backslash$nhow our knowledge of space varies according to geographic location.$\backslash$nBy using data on U.S. city names recalled at 22 test locations, a$\backslash$nmultivariate model of the information surface specific to each test$\backslash$nlocation is calibrated. This model links the probability of a city$\backslash$nbeing recalled from memory to its distance from the test site, its$\backslash$npopulation size, its location with respect to other cities, and whether$\backslash$nor not it is a state capital. The paper then suggests that these$\backslash$nrecall data provide information on spatial knowledge surfaces from$\backslash$nwhich large-scale spatial choices, such as migration destinations,$\backslash$nare made. Results from the analysis lend further evidence to the$\backslash$nidea that spatial knowledge is stored and processed hierarchically$\backslash$nand that individuals underrepresent information in large clusters.$\backslash$nConsequently, the results have important implications for modeling$\backslash$nany spatial behavior based on individuals' spatial information surfaces.$\backslash$nIn particular, the results cast further doubt on the validity of$\backslash$ntraditional large-scale spatial choice frameworks and lend support$\backslash$nto the competing destinations hypothesis.},
