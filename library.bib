journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Climate Change,Disasters,Economics,Human Migration,Humans,Indonesia,Nonlinear Dynamics,Probability},
month = {jul},
number = {27},
pages = {9780--5},
pmid = {24958887},
title = {{Nonlinear permanent migration response to climatic variations but minimal response to disasters.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4103331{\&}tool=pmcentrez{\&}rendertype=abstract http://www.pnas.org/lookup/doi/10.1073/pnas.1317166111},
