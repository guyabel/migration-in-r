journal = {PLoS ONE},
keywords = {Algorithms,Cellular Phone,Cellular Phone: statistics {\&} numerical data,Censuses,Emigrants and Immigrants,Emigrants and Immigrants: statistics {\&} numerical d,Emigration and Immigration,Emigration and Immigration: statistics {\&} numerical,Emigration and Immigration: trends,Geography,Humans,Kenya,Population Density,Population Dynamics,Rural Population,Rural Population: statistics {\&} numerical data,Time Factors,Travel,Travel: statistics {\&} numerical data,Travel: trends,Urban Population,Urban Population: statistics {\&} numerical data},
month = {jan},
