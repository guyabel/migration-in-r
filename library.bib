title = {{Progress toward Global Reduction in Under-Five Mortality: A Bootstrap Analysis of Uncertainty in Millennium Development Goal 4 Estimates}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3519895{\&}tool=pmcentrez{\&}rendertype=abstract http://dx.plos.org/10.1371/journal.pmed.1001355},
