@article{English2017,
abstract = {For many youth, attending university is their first extended time away from home, and uprooting stress can often lead to distress. The present research examines the impact of uprooting stress on educational Chinese migrants and how students cope with the unanticipated pressure of leaving home. Using a sample of recent first-year students, we employed a longitudinal design that allowed us to examine the interaction effect of coping strategies (primary and secondary coping) and stress at time 1 and their impact on anxiety at time 2. Results indicated primary coping exerted an interaction effect on stress, exacerbating the negative effects and leading to more anxiety at time 2, but secondary coping did not. Females also reported lower stress and anxiety. In conclusion, the impact of uprooting stress and coping on psychological symptoms suggests further research on internal Chinese migration should consider the impact of within-culture variation in Mainland China.},
