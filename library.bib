journal = {Environmental Research Letters},
keywords = {89,Shared Socioeconomic Pathways,article is available online,population projections,shared socioeconomic pathways,spatial population,supplementary material for this},
month = {aug},
number = {8},
pages = {084003},
publisher = {IOP Publishing},
title = {{Spatially explicit global population scenarios consistent with the Shared Socioeconomic Pathways}},
url = {http://stacks.iop.org/1748-9326/11/i=8/a=084003 http://stacks.iop.org/1748-9326/11/i=8/a=084003?key=crossref.51e5432142aaf508d829ebd92d31e7ca},
volume = {11},
