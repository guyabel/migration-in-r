title = {{Empirical Prediction Intervals for County Population Forecasts.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2778678{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {28},
year = {2009}
}
@article{davanzo1983rmu,
abstract = {Most previous studies of migration are premised on a one-period model in which the person has a single opportunity to decide to migrate or not, and to select a destination. Yet the demographic literature has long noted that most moves are not people's first moves, but rather are repeated moves -- either onward to new locations or back to places where they lived before (return moves). This paper focuses on repeat moves and inquires into how previous migrants choose among moving on to a new location, returning to a previous location, and staying put. To explain this choice, we broaden the human capital model of migration to incorporate the concepts of location-specific capital and infromation costs. We then test this reformulated model using longitudinal micro data. We find that whereas migration to new locations conforms with previously established relationships, return migration soon after an initial move sometimes contradicts those relationships.},
author = {DaVanzo, Julie},
doi = {10.2307/1935923},
file = {:C$\backslash$:/Users/Guy/Documents/Mendeley Desktop/DaVanzo{\_}1983{\_}Repeat Migration in the United States Who Moves Back and Who Moves On.pdf:pdf},
