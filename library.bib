file = {:C$\backslash$:/Users/Guy/Documents/Mendeley Desktop/Ruktanonchai et al.{\_}2018{\_}Using Google Location History data to quantify fine-scale human mobility.pdf:pdf},
issn = {1476072X},
journal = {International Journal of Health Geographics},
keywords = {GPS tracker data,Human mobility,Mobile phone data},
number = {1},
pages = {1--13},
pmid = {30049275},
publisher = {BioMed Central},
title = {{Using Google Location History data to quantify fine-scale human mobility}},
url = {https://doi.org/10.1186/s12942-018-0150-z},
volume = {17},
year = {2018}
}
