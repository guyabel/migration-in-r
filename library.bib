editor = {Byass, Peter},
file = {:C$\backslash$:/Users/Guy/Documents/Mendeley Desktop/Alkema, New{\_}2012{\_}Progress toward Global Reduction in Under-Five Mortality A Bootstrap Analysis of Uncertainty in Millennium Development.pdf:pdf},
issn = {15491277},
journal = {PLoS Medicine},
month = {dec},
