doi = {10.3390/rs6086844},
file = {:C$\backslash$:/Users/Guy/Documents/Mendeley Desktop/Huang et al.{\_}2014{\_}Application of DMSPOLS nighttime light images A meta-analysis and a systematic literature review.pdf:pdf},
issn = {20724292},
