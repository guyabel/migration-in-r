pmid = {25133549},
title = {{Cross-Checking Different Sources of Mobility Information}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4136853{\&}tool=pmcentrez{\&}rendertype=abstract http://dx.plos.org/10.1371/journal.pone.0105184},
