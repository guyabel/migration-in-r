issn = {00167398},
journal = {Geographical Journal},
keywords = {Component assumptions,Demographic momentum,English populations,Ethnic group populations,Local authority populations,Population projections},
number = {1},
pages = {44--60},
title = {{The demographic drivers of future ethnic group populations for UK local areas 2001-2051}},
