journal = {Earth System Science Data},
number = {2},
pages = {847--856},
title = {{Continuous national gross domestic product (GDP) time series for 195 countries: Past observations (1850-2005) harmonized with future projections according to the Shared Socio-economic Pathways (2006-2100)}},
volume = {10},
year = {2018}
