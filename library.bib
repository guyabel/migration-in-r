issn = {17447941},
journal = {Asia Pacific Journal of Human Resources},
keywords = {Chinese context,ethical leadership,guanxi,organizational citizenship behavior (OCB)},
number = {4},
pages = {408--429},
title = {{How ethical leadership cultivates healthy guanxi to enhance OCB in China}},
volume = {55},
year = {2017}
